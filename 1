#include <stdio.h>
#include <stdlib.h>
int main()
{
    int* m;
    int i=0,j=0,t,p;
    scanf("%d",&p);
    if (p<=0)
    printf("Данные введены некорректно");
    else {
    m=(int *)malloc(p * sizeof(int));
    while(i<p) {
    scanf("%d",&m[i]);
    i++;
    }
    i=0;
    for(j=0;j<p-1;j++) {
    for(i=0;i<p-1-j;i++) {
        if (m[i]>m[i+1]) {
            t=m[i];
            m[i]=m[i+1];
            m[i+1]=t;
        }
    }
    i=0;
    }
    while(i<p) {
    printf("%d ",m[i]);
    i++;
    }
    free(m);
    }
    return 0;
}
